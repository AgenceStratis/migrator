{
  "alias": "migrator.phar",
  "chmod": "0755",
  "directories": [
    "src"
  ],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "ddeboer/data-import/tests",
        "phpoffice/phpexcel/Examples",
        "phpoffice/phpexcel/unitTests",
        "symfony/yaml/Tests",
        "dfridrich/php-mime-type/Tests",
        "hoa/consistency/Test",
        "hoa/event/Test",
        "hoa/exception/Test",
        "hoa/file/Test",
        "hoa/iterator/Test",
        "hoa/protocol/Test",
        "hoa/stream/Test"
      ],
      "in": "vendor"
    }
  ],
  "main": "bin/main.php",
  "output": "migrator.phar",
  "stub": true
}